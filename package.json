{
  "name": "MaterialHubApp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "exceljs": "^4.4.0",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds run",
    "watch-materialhub": "cds watch --open materialhub/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "auth": "mocked"
      },
      "watch": {
        "exclude": [
          "node_modules",
          "dist",
          "test"
        ]
      },
      "destinations": true,
      "html5-repo": true,
      "workzone": true
    }
  },
  "sapux": [
    "app/materialhub"
  ],
  "engines": { "node": ">=18 <21" }
}
